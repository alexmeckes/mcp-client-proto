# Copy this file to .env and fill in your actual values

# Anthropic API Key (get from https://console.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# MCPD Configuration (optional, for local MCP daemon)
MCPD_BASE_URL=http://localhost:8090/api/v1

# Composio API Key (get from https://app.composio.dev)
COMPOSIO_API_KEY=your_composio_api_key_here

# Frontend URL for CORS (update for production)
FRONTEND_URL=http://localhost:5173

# Cloud Mode (set to true in production)
CLOUD_MODE=false
